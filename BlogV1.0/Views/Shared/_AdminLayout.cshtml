
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="tr">
<head>
  <meta charset="utf-8"/>
  <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no"/>
  <meta name="description" content=""/>
  <meta name="author" content=""/>
  <title>Alparslan Blogger</title>
  <!-- loader-->
 @*<link href="~/AdminTema/assets/css/pace.min.css" rel="stylesheet"/>*@
  <script src="~/AdminTema/assets/js/pace.min.js"></script>
  <!--favicon-->
  <link rel="icon" href="~/AdminTema/assets/images/favicon.ico" type="image/x-icon">
  <!-- Vector CSS -->
  <link href="~/AdminTema/assets/plugins/vectormap/jquery-jvectormap-2.0.2.css" rel="stylesheet"/>
  <!-- simplebar CSS-->
  <link href="~/AdminTema/assets/plugins/simplebar/css/simplebar.css" rel="stylesheet"/>
  <!-- Bootstrap core CSS-->
  <link href="~/AdminTema/assets/css/bootstrap.min.css" rel="stylesheet"/>
  <!-- animate CSS-->
  <link href="~/AdminTema/assets/css/animate.css" rel="stylesheet" type="text/css"/>
  <!-- Icons CSS-->
  <link href="~/AdminTema/assets/css/icons.css" rel="stylesheet" type="text/css"/>
  <!-- Sidebar CSS-->
  <link href="~/AdminTema/assets/css/sidebar-menu.css" rel="stylesheet"/>
  <!-- Custom Style-->
  <link href="~/AdminTema/assets/css/app-style.css" rel="stylesheet"/>
  
</head>

<body class="bg-theme bg-theme2">
 
<!-- Start wrapper-->
 <div id="wrapper">
 
  <!--Start sidebar-wrapper-->
   @await Html.PartialAsync("Sidebar")
   <!--End sidebar-wrapper-->

<!--Start topbar header-->
   @await Html.PartialAsync("Navbar")
<!--End topbar header-->

<div class="clearfix"></div>
	
  <div class="content-wrapper">
    <div class="container-fluid">
  <!--start Row-->
	@RenderBody()	  
	<!--End Row-->
	<!--start overlay-->
		  <div class="overlay toggle-menu"></div>
		<!--end overlay-->	
    </div>
 
    </div><!--End content-wrapper-->
   <!--Start Back To Top Button-->
    <a href="javaScript:void();" class="back-to-top"><i class="fa fa-angle-double-up"></i> </a>
    <!--End Back To Top Button-->
	
	<!--Start footer-->
	<footer class="footer">
      <div class="container">
        <div class="text-center">
         Telif Hakkı Tasarım Ve Kodlama | Alparslan Akbaş Aittir.&copy;<script>document.write(new Date().getFullYear());</script>
        </div>
      </div>
    </footer>
	<!--End footer-->
	
  <!--start color switcher-->
   <div class="right-sidebar">
    <div class="switcher-icon">
      <i class="zmdi zmdi-settings zmdi-hc-spin"></i>
    </div>
    <div class="right-sidebar-content">

      <p class="mb-0">Metalimsi Renkler</p>
      <hr>
      
      <ul class="switcher">
        <li id="theme1" onclick="renkDegistir('theme1')"></li>
        <li id="theme2" onclick="renkDegistir('theme2')"></li>
        <li id="theme3" onclick="renkDegistir('theme3')"></li>
        <li id="theme4" onclick="renkDegistir('theme4')"></li>
        <li id="theme5" onclick="renkDegistir('theme5')"></li>
        <li id="theme6" onclick="renkDegistir('theme6')"></li>
      </ul>

      <p class="mb-0">Sade Renkler</p>
      <hr>
      
      <ul class="switcher">
        <li id="theme7"></li>
        <li id="theme8"></li>
        <li id="theme9"></li>
        <li id="theme10"></li>
        <li id="theme11"></li>
        <li id="theme12"></li>
		<li id="theme13"></li>
        <li id="theme14"></li>
        <li id="theme15"></li>
      </ul>
      
     </div>
   </div>
  <!--end color switcher-->
   
  </div><!--End wrapper-->

  <!-- Bootstrap core JavaScript-->
  <script src="~/AdminTema/assets/js/jquery.min.js"></script>
  <script src="~/AdminTema/assets/js/popper.min.js"></script>
  <script src="~/AdminTema/assets/js/bootstrap.min.js"></script>
	
 <!-- simplebar js -->
  <script src="~/AdminTema/assets/plugins/simplebar/js/simplebar.js"></script>
  <!-- sidebar-menu js -->
  <script src="~/AdminTema/assets/js/sidebar-menu.js"></script>
  <!-- loader scripts -->
  <script src="~/AdminTema/assets/js/jquery.loading-indicator.js"></script>
  <!-- Custom scripts -->
  <script src="~/AdminTema/assets/js/app-script.js"></script>
  <!-- Chart js -->
  
  <script src="~/AdminTema/assets/plugins/Chart.js/Chart.min.js"></script>
 
  <!-- Index js -->
  <script src="~/AdminTema/assets/js/index.js"></script>

  <script>

function renkDegistir(renk)
{
    document.body.style.background=renk;
}

</script>
</body>
</html>
